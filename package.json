{
  "name": "bando-nuibaden",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/sync-assets.js && vite build",
    "build:check": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:functions": "node scripts/test-functions.js",
    "netlify:dev": "netlify dev",
    "netlify:build": "netlify build",
    "netlify:deploy": "netlify deploy --prod",
    "generate:component": "node scripts/generate-component.js",
    "generate:docs": "node scripts/generate-docs.js",
    "validate:components": "node scripts/validate-components.js",
    "optimize:images": "node scripts/optimize-images.js",
    "build:optimize": "node scripts/build-optimize.js",
    "build:analyze": "ANALYZE=true npm run build",
    "setup:testing": "node scripts/setup-testing.js",
    "cli:help": "node scripts/cli-help.js",
    "fix:mime": "node scripts/fix-mime-types.js",
    "docs": "npm run generate:docs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:visual": "playwright test",
    "test:visual:ui": "playwright test --ui",
    "prepare": "[ \"$HUSKY\" = \"0\" ] || husky install"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.28.0",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
